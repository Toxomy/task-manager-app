{
  "name": "task-manager-app",
  "version": "1.0.0",
  "description": "A full-stack task manager application",
  "scripts": {
    "install:all": "npm install && cd FrontEnd-Git && npm install && cd ../Backend-Git && npm install",
    "start:frontend": "cd FrontEnd-Git && npm start",
    "start:backend": "cd Backend-Git && npm start",
    "dev": "concurrently \"npm run start:frontend\" \"npm run start:backend\""
  },
  "keywords": [
    "task-manager",
    "full-stack",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
} 